  },
  "homepage": "https://github.com/LGG233/timekeeper#readme",
  "dependencies": {
    "axios": "^0.21.2",
    "body-parser": "^1.19.1",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "if-env": "^1.0.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "router": "^1.3.6",
    "sequelize": "^5.22.5"
  }
